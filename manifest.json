{
    "name": "Tab Repeater!",
    "description": "Simple tab repeater Google Chrome Extension to open specific url multiple times, useful for performance testing!",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
      "48": "/assets/icons/bell.png"
    },

    "action": {
        "default_popup": "popup.html",
        "default_icon": {
        "48": "/assets/icons/bell.png"
        }
      }, 
      "permissions": [
        "tabs",
        "notifications"
      ],

      "background": {
        "service_worker": "background.js"
      }
  }
  